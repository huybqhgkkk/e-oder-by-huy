{
  "rewrites": [
    {
      "source": "/api/v1/:match*",
      "destination": "http://103.179.191.177:8080/api/v1/:match*"
    },
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
